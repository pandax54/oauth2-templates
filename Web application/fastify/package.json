{
  "name": "@fernandapanda/oauth",
  "version": "0.0.1",
  "main": "./build/src/server.js",
  "author": "@pandax54",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "nodemon --delay 200ms --exec 'npm start'",
    "infra": "docker-compose up --detach --force-recreate",
    "infra:stop": "docker-compose down",
    "db:migration": "NODE_ENV=local knex migrate:make --knexfile src/db/knexfile.ts rename-me",
    "db:migrate": "knex migrate:latest --knexfile src/db/knexfile.ts && knex seed:run --knexfile src/db/knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile src/db/knexfile.ts",
    "db:seed": "knex seed:run --knexfile src/db/knexfile.ts",
    "db:makeseed": "NODE_ENV=local knex seed:make --knexfile src/db/knexfile.ts rename-me",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src}/**/*.ts\""
  },
  "dependencies": {
    "@fastify/cors": "^8.2.1",
    "@fastify/multipart": "^7.6.0",
    "@fastify/rate-limit": "^8.0.0",
    "@fastify/static": "^6.10.1",
    "@fastify/swagger": "^8.4.0",
    "@sinclair/typebox": "^0.29.6",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "camelcase-keys": "^8.0.2",
    "dotenv": "^16.1.0",
    "env-schema": "^5.0.0",
    "express": "^4.18.2",
    "fastify": "^4.17.0",
    "fastify-plugin": "^4.5.0",
    "fastify-raw-body": "^4.2.1",
    "handlebars": "^4.7.7",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.2",
    "make-promises-safe": "^5.1.0",
    "objection": "^3.0.1",
    "openid-client": "^5.4.2",
    "pg": "^8.11.0",
    "pino": "^7.10.0",
    "pino-pretty": "^10.0.1",
    "query-string": "^7.0.1",
    "snakecase-keys": "^5.4.6",
    "throng": "^5.0.0",
    "type-fest": "^4.0.0",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.26",
    "@types/pino": "^7.0.5",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-json-schema": "^6.1.0",
    "json-schema-faker": "^0.5.0-rcv.42",
    "json-schema-to-typescript": "^13.0.2",
    "nock": "^13.3.0",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "ts-essentials": "^9.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.5.4"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "singleQuote": true,
    "tabWidth": 2
  },
  "nodemonConfig": {
    "watch": "src/",
    "ext": "js,json,ts"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.5.1"
  }
}
